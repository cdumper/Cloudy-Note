<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view"
	xmlns:ck="urn:import:com.axeiya.gwtckeditor.client">
	<ui:style>
		
	</ui:style>
	<cn:Container ui:field="container" height="100%" width="100%">
		<g:DockLayoutPanel unit="PCT" ui:field="content" height="100%" width="100%">
			<g:north size="6">
				<g:Button ui:field="doneButton">Done</g:Button>
			</g:north>
			<g:center>
				<g:VerticalPanel ui:field="topPanel">
					<g:TextBox ui:field="title"></g:TextBox>
					<g:ListBox ui:field="notebook"></g:ListBox>
					<g:Anchor ui:field="attach">Attach files</g:Anchor>
					<ck:CKEditor ui:field='ckeditor' />
				</g:VerticalPanel>
			</g:center>
		</g:DockLayoutPanel>
	</cn:Container>
</ui:UiBinder> 