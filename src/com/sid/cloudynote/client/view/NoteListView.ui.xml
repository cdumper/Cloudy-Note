<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view">
	<ui:style>
		.container {
			border: 1px solid #999;
		}
	</ui:style>
	<cn:Container ui:field="container" addStyleNames="{style.container}">
		<g:HTMLPanel width="100%" height="6%">
			<g:TextBox ui:field="searchBox">Search notes</g:TextBox>
		</g:HTMLPanel>
		<g:HTMLPanel width="100%">
			<g:Label ui:field="label"></g:Label>
		</g:HTMLPanel>
		<cn:ShowMorePagerPanel ui:field='pagerPanel'
			width="100%"/>
	</cn:Container>
</ui:UiBinder> 