<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view">
	<ui:style>
		.container {
			border: 1px solid #999;
		}
	</ui:style>
	<cn:Container ui:field="container" addStyleNames="{style.container}">
		<g:DockLayoutPanel unit='PCT'>
			<g:north size='10'>
				<g:VerticalPanel width="100%" height="100%">
					<g:TextBox ui:field="searchBox">Search notes</g:TextBox>
					<g:Label ui:field="label"></g:Label>
				</g:VerticalPanel>
			</g:north>
			<g:center>
				<cn:ShowMorePagerPanel ui:field='pagerPanel'
					width="100%" />
			</g:center>
		</g:DockLayoutPanel>
	</cn:Container>
</ui:UiBinder> 