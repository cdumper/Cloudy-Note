<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view"
	xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.container {
			border: 1px solid #999;
		}
	</ui:style>
	<cn:Container ui:field="container" addStyleNames="{style.container}">
		<g:HTMLPanel ui:field="content" height="100%" width="100%">
		</g:HTMLPanel>
	</cn:Container>
</ui:UiBinder> 