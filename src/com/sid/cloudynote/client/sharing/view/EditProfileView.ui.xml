<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view">
	<ui:style
		type="com.sid.cloudynote.client.sharing.view.EditProfileView.Style"
		src="profile.css">
			
		</ui:style>
	<cn:Container ui:field="container" addStyleNames="{style.container}">
		<g:HTMLPanel ui:field="content" addStyleNames="{style.centerPanel}">
			<div class="{style.panel}">
				<div class="{style.titlePanel}">
					<g:Label ui:field="title" addStyleNames="{style.title}"
						text="User Profile"></g:Label>
					<g:Button ui:field="saveButton" addStyleNames="{style.button}"
						text="Save" />
				</div>
				<div class="{style.row}">
					<g:Label text="Full Name" addStyleNames="{style.sublabel}"></g:Label>
					<g:TextBox ui:field="fullName" addStyleNames="{style.input}"></g:TextBox>
				</div>
				<div class="{style.row}">
					<g:Label text="Nick Name" addStyleNames="{style.sublabel}"></g:Label>
					<g:TextBox ui:field="nickName" addStyleNames="{style.input}"></g:TextBox>
				</div>
				<div class="{style.row}">
					<g:Label text="Email" addStyleNames="{style.sublabel}"></g:Label>
					<g:TextBox ui:field="email" addStyleNames="{style.input}"></g:TextBox>
				</div>
			</div>
		</g:HTMLPanel>
	</cn:Container>
</ui:UiBinder> 