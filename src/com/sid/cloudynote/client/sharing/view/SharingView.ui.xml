<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s='urn:import:com.sid.cloudynote.client.sharing.view'>
	<ui:style>
	
	</ui:style>
	<g:DockLayoutPanel unit='PCT' ui:field="dockLayoutPanel">
		<g:north size='6'>
			<s:SharingSearchView ui:field='searchView' />
		</g:north>
		<g:west size='12'>
			<s:GroupView ui:field='groupView' />
		</g:west>
		<g:center>
			<g:LayoutPanel width="100%" height="100%">
				<g:layer left="0" width="88%">
					<s:SharingNoteListView ui:field='noteListView' />
				</g:layer>
				<g:layer left="25%" width="63%">
					<s:SharingNoteView ui:field='noteView' />
				</g:layer>
				<g:layer width="12%" right="0">
					<s:FriendView ui:field='friendView' />
				</g:layer>
			</g:LayoutPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 