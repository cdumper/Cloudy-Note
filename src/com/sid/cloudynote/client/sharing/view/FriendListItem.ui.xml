<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view">
	<ui:style>
		table {
			border: 5px;
			width: 100%;
			height: 40px;
			border: 0px solid grey;
			border-bottom: 1px solid grey;
			padding: 10px;
		}
	</ui:style>
	<cn:Container ui:field="container">
		<g:HTMLPanel ui:field="content">
			<table>
				<tr>
					<td rowspan="2" width="10%">user image profile</td>
					<td width="20%">
						<g:Button ui:field="userLink">user name</g:Button>
					</td>
					<td rowspan="2" width="10%">
						<g:Label ui:field="joinSinceLabel">joined since</g:Label>
					</td>
					<td rowspan="2">
						<g:Label ui:field="totalNotesLabel">total notes</g:Label>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="groupLabel">group</g:Label>
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</cn:Container>
</ui:UiBinder> 