<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<cn:Container ui:field="container">
		<g:HorizontalPanel ui:field="content">
			<g:Button ui:field='newNotebook' text='New Notebook'></g:Button>
			<g:Button ui:field='newNote' text='New Note' />
			<g:TextBox ui:field='searchField' />
			<g:Button ui:field='edit' text='Edit'></g:Button>
		</g:HorizontalPanel>
	</cn:Container>
</ui:UiBinder> 