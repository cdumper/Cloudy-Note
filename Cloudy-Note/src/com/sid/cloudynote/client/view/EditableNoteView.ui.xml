<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view"
	xmlns:ck="urn:import:com.axeiya.gwtckeditor.client">
	<ui:style>
	</ui:style>

	<g:DockLayoutPanel unit="PCT">
		<g:north size="20">
			<g:VerticalPanel ui:field="topPanel">
				<g:TextBox ui:field="title"></g:TextBox>
				<g:ListBox ui:field="notebook"></g:ListBox>
				<g:Anchor ui:field="attach">Attach files</g:Anchor>
			</g:VerticalPanel>
		</g:north>
		<g:center>
			<ck:CKEditor ui:field='ckeditor' />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 