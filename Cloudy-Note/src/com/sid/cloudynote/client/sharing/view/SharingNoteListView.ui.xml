<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cn="urn:import:com.sid.cloudynote.client.view"
	xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.container {
			border: 1px solid #999;
		}
	</ui:style>
	<cn:Container ui:field="container" addStyleNames="{style.container}">
		<g:DeckPanel>
			<g:TabLayoutPanel barUnit='EM' barHeight='2' height="100%"
				ui:field="tabPanel">
				<g:tab>
					<g:header size='7'>Notes</g:header>
					<g:HTMLPanel ui:field="tabNotes">
					</g:HTMLPanel>
				</g:tab>
				<g:tab>
					<g:header size='7'>Tags</g:header>
					<g:HTMLPanel ui:field="tabTags">
						<g:Label>tags</g:Label>
					</g:HTMLPanel>
				</g:tab>
				<g:tab>
					<g:header size='7'>Groups</g:header>
					<g:HTMLPanel ui:field="tabGroups">
						<g:Label>groups</g:Label>
					</g:HTMLPanel>
				</g:tab>
			</g:TabLayoutPanel>
			<!-- <g:LayoutPanel>
				<g:layer >
				</g:layer>
				<g:layer >
				</g:layer>
			</g:LayoutPanel> -->
		</g:DeckPanel>
	</cn:Container>
</ui:UiBinder> 