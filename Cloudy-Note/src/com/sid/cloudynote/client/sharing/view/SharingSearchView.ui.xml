<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cn="urn:import:com.sid.cloudynote.client.view">
	<ui:style>
		.container {
			border: 1px solid #999;
		}
	</ui:style>
	<cn:Container ui:field="container">
		<g:HorizontalPanel ui:field="content" spacing="5" verticalAlignment="ALIGN_MIDDLE">
			<g:TextBox ui:field='searchField' alignment="CENTER" />
			<g:ListBox ui:field='sortBy'/>
			<g:ListBox ui:field='view'/>
			<g:ListBox ui:field='settings'/>
		</g:HorizontalPanel>
	</cn:Container>
</ui:UiBinder> 